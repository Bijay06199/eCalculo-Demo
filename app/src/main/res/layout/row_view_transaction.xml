<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lLDataLayout"
    android:layout_width="wrap_content"
    android:layout_height="@dimen/_40sdp"
    android:orientation="horizontal">

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVNepaliDate"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:textAlignment="center"
        android:textStyle="bold" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVEngDate"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:textAlignment="center"
        android:textStyle="bold" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVBillNumber"
        android:layout_width="@dimen/_120sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:textAlignment="center"
        android:textStyle="bold" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVCustomer"
        android:layout_width="@dimen/_200sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:paddingStart="@dimen/_5sdp"
        android:paddingLeft="@dimen/_5sdp"
        android:textAlignment="viewStart"
        android:textStyle="bold"
        tools:ignore="RtlSymmetry" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVAddress"
        android:layout_width="@dimen/_200sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:paddingStart="@dimen/_5sdp"
        android:paddingLeft="@dimen/_5sdp"
        android:textAlignment="viewStart"
        android:textStyle="bold"
        tools:ignore="RtlSymmetry" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVStore"
        android:layout_width="@dimen/_200sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:paddingStart="@dimen/_5sdp"
        android:paddingLeft="@dimen/_5sdp"
        android:textAlignment="viewStart"
        android:textStyle="bold"
        tools:ignore="RtlSymmetry" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVSubTotal"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="match_parent"
        android:gravity="center|end"
        android:paddingStart="@dimen/_5sdp"
        android:paddingLeft="@dimen/_5sdp"
        android:textAlignment="viewEnd"
        android:textStyle="bold"
        tools:ignore="RtlSymmetry" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVNonTaxable"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:paddingEnd="@dimen/_5sdp"
        android:paddingRight="@dimen/_5sdp"
        android:textAlignment="viewEnd"
        android:textStyle="bold"
        tools:ignore="RtlCompat,RtlSymmetry" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVTaxable"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:paddingEnd="@dimen/_5sdp"
        android:paddingRight="@dimen/_5sdp"
        android:textAlignment="viewEnd"
        android:textStyle="bold"
        tools:ignore="RtlCompat,RtlSymmetry" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVVat"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:paddingEnd="@dimen/_5sdp"
        android:paddingRight="@dimen/_5sdp"
        android:textAlignment="viewEnd"
        android:textStyle="bold"
        tools:ignore="RtlCompat,RtlSymmetry" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <TextView
        android:id="@+id/tVTotal"
        android:layout_width="@dimen/_150sdp"
        android:layout_height="match_parent"
        android:gravity="center|start"
        android:paddingEnd="@dimen/_5sdp"
        android:paddingRight="@dimen/_5sdp"
        android:textAlignment="viewEnd"
        android:textStyle="bold"
        tools:ignore="RtlCompat,RtlSymmetry" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />

    <LinearLayout
        android:layout_width="@dimen/_105sdp"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/iVEdit"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_15sdp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/ic_baseline_edit_24"
            android:tint="@color/colorPrimary" />


        <View
            android:layout_width="@dimen/_1sdp"
            android:layout_height="match_parent"
            android:background="?android:attr/listDivider" />


        <ImageView
            android:id="@+id/iVDelete"
            android:layout_width="@dimen/_25sdp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_15sdp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/ic_delete_white"
            android:tint="@color/colorPrimary" />

    </LinearLayout>

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider" />


    <ImageView
        android:id="@+id/iVViewVoucher"
        android:layout_width="@dimen/_20sdp"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_20sdp"
        android:contentDescription="@string/app_name"
        android:src="@drawable/ic_baseline_eye_24"
        android:tint="@color/colorPrimary"
        android:visibility="gone" />

    <View
        android:layout_width="@dimen/_1sdp"
        android:layout_height="match_parent"
        android:background="?android:attr/listDivider"
        android:visibility="gone" />

</LinearLayout>

